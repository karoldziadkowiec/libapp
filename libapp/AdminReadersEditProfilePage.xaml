        <Rectangle HorizontalAlignment="Left" Height="3" Margin="338,309,0,0" Stroke="#FF641717" VerticalAlignment="Top" Width="150" Fill="#FF641717"/>
        <Rectangle HorizontalAlignment="Left" Height="3" Margin="519,309,0,0" Stroke="#FF641717" VerticalAlignment="Top" Width="150" Fill="#FF641717"/>
        <Rectangle HorizontalAlignment="Left" Height="3" Margin="703,309,0,0" Stroke="#FF641717" VerticalAlignment="Top" Width="175" Fill="#FF641717"/>
