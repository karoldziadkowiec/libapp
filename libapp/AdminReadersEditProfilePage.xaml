        <TextBox x:Name="address_textbox" HorizontalAlignment="Left" Margin="758,416,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="302" Height="36" FontFamily="Comic Sans MS" FontSize="24" VerticalContentAlignment="Center" BorderBrush="#FF641717"/>
        <DatePicker x:Name="birthday_datepicker" HorizontalAlignment="Left" Margin="614,416,0,0" VerticalAlignment="Top" Height="36" Width="125" FontFamily="Comic Sans MS" FontSize="17" FirstDayOfWeek="Monday" TabIndex="36"/>
        <Label Content="Name" HorizontalAlignment="Left" Margin="338,261,0,0" VerticalAlignment="Top" Height="51" Width="150" FontFamily="Comic Sans MS" FontSize="24" Foreground="#FF641717"/>
        <Label Content="Surname" HorizontalAlignment="Left" Margin="519,261,0,0" VerticalAlignment="Top" Height="51" Width="150" FontFamily="Comic Sans MS" FontSize="24" Foreground="#FF641717"/>
        <Label Content="PESEL number" HorizontalAlignment="Left" Margin="703,261,0,0" VerticalAlignment="Top" Height="51" Width="175" FontFamily="Comic Sans MS" FontSize="24" Foreground="#FF641717"/>
