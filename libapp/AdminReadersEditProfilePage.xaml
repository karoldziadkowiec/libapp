        <Label Content="E-mail" HorizontalAlignment="Left" Margin="338,360,0,0" VerticalAlignment="Top" Height="51" Width="256" FontFamily="Comic Sans MS" FontSize="24" Foreground="#FF641717"/>
        <Label Content="Birthday" HorizontalAlignment="Left" Margin="614,360,0,0" VerticalAlignment="Top" Height="51" Width="125" FontFamily="Comic Sans MS" FontSize="24" Foreground="#FF641717"/>
        <Label Content="Address" HorizontalAlignment="Left" Margin="758,360,0,0" VerticalAlignment="Top" Height="51" Width="302" FontFamily="Comic Sans MS" FontSize="24" Foreground="#FF641717"/>
