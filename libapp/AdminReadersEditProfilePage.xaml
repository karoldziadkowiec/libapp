        <TextBox x:Name="surname_textbox" HorizontalAlignment="Left" Margin="519,317,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" Height="36" FontFamily="Comic Sans MS" FontSize="24" VerticalContentAlignment="Center" BorderBrush="#FF641717"/>
